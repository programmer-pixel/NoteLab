{
  "name": "NoteLab",
  "description": "A Google Chrome extension that makes taking notes 100 times easier.",
  "version": "0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "notes.html",
    "default_icon": {
      "48": "logo.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["init.js"]
    }
  ]
}